{
  "name": "ee-site-example",
  "private": true,
  "type": "module",
  "scripts": {
    "build:css": "postcss ./node_modules/@wcs-mn/ee-core/src/assets/css/global/global.css -o ./src/assets/css/global.css",
    "build": "npm run build:css && eleventy",
    "start": "npm run build:css && eleventy --serve",
    "clean": "rm -rf dist src/assets/css/global.css"
  },
  "dependencies": {
    "@11ty/eleventy": "^3.1.0",
    "@wcs-mn/ee-core": "github:REPLACE_WITH_YOUR_GITHUB_OR_NPM_REF"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.0",
    "cssnano": "^7.0.0",
    "postcss": "^8.5.0",
    "postcss-cli": "^11.0.0",
    "postcss-import": "^16.0.0",
    "postcss-import-ext-glob": "^2.1.0",
    "tailwindcss": "^3.4.0"
  }
}